  agent any
  stages {
    stage("build") {
       steps {
          echo 'buidling the application...'
       } 
    }
    stage("test") {
       steps {
          echo 'testing the application...'
       } 
    }
    stage("deploy") {
       steps {
          echo 'deploying the application...'
       } 
    }
    
  }
}
